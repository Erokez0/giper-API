{
    "include": ["./src"],
    "compilerOptions": {
      "module": "commonjs",
      "target": "ES2020",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "baseUrl": "./src",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "outDir": "./dist",
    },
}